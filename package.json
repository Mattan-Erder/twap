{
  "name": "@orbs-network/twap",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbs-network/twap.git"
  },
  "license": "MIT",
  "dependencies": {
    "@defi.org/web3-candies": "3.x"
  },
  "scripts": {
    "clean": "rm -rf artifacts cache typechain-hardhat",
    "prettier": "prettier --write '{test,src,contracts}/**/*.{ts,js,json,sol}'",
    "typechain": "npm run clean && hardhat typechain",
    "prebuild": "npm run prettier && npm run typechain",
    "build": "hardhat compile && solhint 'contracts/**/*.sol'",
    "test": "npm run test:eth && npm run test:poly",
    "test:eth": "BLOCK=14908980 DEBUG=web3-candies hardhat test --logs",
    "test:poly": "NETWORK=POLY BLOCK=29194866 DEBUG=web3-candies hardhat test --logs"
  },
  "prettier": {
    "printWidth": 120
  }
}
